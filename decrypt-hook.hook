#!/usr/bin/ash
run_hook() {
    modprobe -a -q dm-crypt >/dev/null 2>&1
    [ "${quiet}" = "y" ] && CSQUIET=">/dev/null"
        export TERMINFO=/usr/lib/terminfo
	bash /usr/bin/decrypt open

}

